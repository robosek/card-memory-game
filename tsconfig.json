{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "jsx": "preserve",
    "newLine": "LF",
    "lib": [
      "dom",
      "es2017"
    ],
    "moduleResolution": "node",
    "importHelpers": true,
    "sourceMap": true,
    "inlineSources": true,
    "jsxFactory": "jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src"
  ]
}
